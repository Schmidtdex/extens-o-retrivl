{
    "manifest_version": 3,
    "name": "Retrivl",
    "description": "Sem por enquanto",
    "version": "1.1",
    "permissions": [
        "scripting",
        "activeTab",
        "tabs"
    ],
        "host_permissions": [
        "http://localhost:8502/*",
        "https://localhost:8502/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}